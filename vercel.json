{
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/apropos",
      "destination": "/a-propos",
      "permanent": true
    },
    {
      "source": "/Apropos",
      "destination": "/a-propos",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/icon_new.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
